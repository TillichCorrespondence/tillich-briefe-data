     <text type="postcard">
          <body>
               <div type="writingSession">
                    <pb n="1"/>
                    <opener>
                         <salute>L. <rs ref="#tillich_person_id__1928" type="person"
                              >Paul</rs>!</salute>
                    </opener>
                    <p><foreign xml:lang="lat">Gratulor</foreign>. <rs type="letter" ref="L00183">Brief</rs> ist schon in Arbeit, habe
                         aber hier bei <rs ref="#tillich_person_id__2178" type="person">Wilm</rs>
                         keine <hi rend="u">Zeit</hi> dazu. Frdl. </p>
                    <closer>Gruß an die Deinigen u. <rs
                              ref="#tillich_person_id__1295" type="person">Meinhof</rs> Dein <signed><rs ref="#tillich_person_id__788" type="person">G. v.
                                   Hanffstengel</rs></signed>
                    </closer>
                    <postscript>
                         <p>auch <foreign xml:lang="lat">gratulor</foreign>. <rs
                                   ref="#tillich_person_id__2178" type="person">Wilm</rs>.</p>
                    </postscript>
               </div>
          </body>
     </text>
